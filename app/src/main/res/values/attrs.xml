<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="CircleFillingProgressView">
		<attr name="circle_progress" format="integer"/>
		<attr name="circle_max" format="integer"/>
		<attr name="circle_unfinished_color" format="color"/>
		<attr name="circle_finished_color" format="color"/>
		<attr name="circle_text_size" format="dimension"/>
		<attr name="circle_text_color" format="color"/>
		<attr name="circle_suffix_text" format="string"/>
		<attr name="circle_progress_text_typeface" format="string"/>
		<attr name="circle_hide_progress_text" format="boolean"/>
		<attr name="circle_custom_start_angle" format="integer"/>
		<attr name="circle_fill_from" format="enum">
			<enum name="bottom" value="0" />
			<enum name="left" value="1" />
			<enum name="top" value="2" />
			<enum name="right" value="3" />
			<enum name="bottomLeft" value="4" />
			<enum name="topLeft" value="5" />
			<enum name="topRight" value="6" />
			<enum name="bottomRight" value="7" />
			<enum name="customStartAngle" value="8" />
		</attr>
	</declare-styleable>
	<string name="app_name">ProgressViews</string>
</resources>